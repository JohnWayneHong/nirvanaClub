                    android:textSize="14sp"

                    tools:text="244776740213121278" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="25dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/link_dlh"
                    android:textColor="@color/gray_33"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="110dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{data.linkText}"
                    android:textColor="@color/gray_99"
                    android:textSize="14sp"

                    tools:text="244776740213121278" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="25dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/location_dlh"
                    android:textColor="@color/gray_33"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="110dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{data.locationText}"
                    android:textColor="@color/gray_99"
                    android:textSize="14sp"

                    tools:text="244776740213121278" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="25dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/address_dlh"
                    android:textColor="@color/gray_33"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="110dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{data.detailedAddress}"
                    android:textColor="@color/gray_99"
                    android:textSize="14sp"

                    tools:text="244776740213121278" />

            </RelativeLayout>

        </LinearLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:text="@string/order_stock_out_product_info"
            android:textColor="@color/gray_33"
            android:textSize="14sp" />
    </LinearLayout>

</layout>